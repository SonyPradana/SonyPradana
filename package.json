{
  "name": "simpuslerep",
  "version": "1.1.0",
  "description": "sistem informasi managemen puskesmas lerep (unofficial)",
  "main": "/lib/js/app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "sass" : "npm run sass-full && npm run sass-cards && npm run sass-style && npm run sass-widget",
    "sass-full": "sass --no-source-map --style=compressed lib/scss/full.style.scss public/lib/css/ui/v1.1/full.style.css",
    "sass-cards": "sass --no-source-map --style=compressed lib/scss/cards.scss public/lib/css/ui/v1.1/cards.css",
    "sass-style": "sass --no-source-map --style=compressed lib/scss/style.scss public/lib/css/ui/v1.1/style.css",
    "sass-widget": "sass --no-source-map --style=compressed lib/scss/widgets.scss public/lib/css/ui/v1.1/widgets.css",
    "terser-index": "terser -c -m -- lib/js/index.js > public/lib/js/index.min.js",
    "publish": "git checkout master && git merge feature && git push github master && git ftp push"
  },
  "keywords": [
    "simpus",
    "public",
    "healt",
    "medical",
    "record",
    "puskesmas"
  ],
  "author": "angger",
  "license": "ISC"
}
